<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="Scheduler">
  <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name="com.google.gwt.user.User"/>
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="net.customware.gwt.presenter.Presenter"/>
    <inherits name="org.cobogw.gwt.user.Button"/>
    <inherits name="org.openxdata.server.admin.Model"/>
    <inherits name="org.openxdata.modules.workflows.model.Model"/>
    <inherits name="net.sf.gilead.Gilead4Gwt"/>
    
  <!-- Other module inherits                                      -->
  <!-- Specify the app entry point class.                         -->
    <entry-point class="org.yawl.schedulingservice.client.Sheduler"/>
  <!-- Specify the paths for translatable code                    -->
    <source path="client"/>
    <source path="shared"/>
    <set-property name="locale" value="default"/>
    <set-property name="user.agent" value="gecko1_8"/>
    <extend-property name="locale" values="en"/>
    <set-property name="locale" value="en"/>
    <stylesheet src="scheduler.css"/>
</module>
