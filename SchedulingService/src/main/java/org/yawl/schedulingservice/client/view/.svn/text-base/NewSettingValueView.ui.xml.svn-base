<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
           <table>
                <tr>
                    <td>Name</td>
                    <td><g:TextBox ui:field="txtName" width="100%"/></td>
                </tr>
                <tr>
                    <td>Value</td>
                    <td><g:TextBox ui:field="txtValue" width="100%"/></td>
                </tr>
                <tr>
                    <td>Description</td>
                    <td><g:TextBox ui:field="txtDesc" width="100%"/></td>
                </tr>
               
                <tr>
                    <td><g:Button ui:field="btnOk">Ok</g:Button></td>
                    <td><g:Button ui:field="btnCancel">Cancel</g:Button></td>
                </tr>
            </table>
	</g:HTMLPanel>
</ui:UiBinder>