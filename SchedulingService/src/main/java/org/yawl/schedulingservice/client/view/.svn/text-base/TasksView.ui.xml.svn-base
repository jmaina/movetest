<?xml version="1.0" encoding="UTF-8"?>

<!--<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">-->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:s="urn:import:org.cobogw.gwt.user.client.ui">

    <g:HTMLPanel ui:field="sp" width="100%" height="500px">
        <g:VerticalPanel ui:field="vp" spacing="0" width="100%" >
            <g:HTMLPanel  width="100%">
                <div class="toolBar-Header">
                    <div style="float: left; display: inline-block">
                        <s:ButtonBar ui:field="btnBar"/>
                    </div>
                    <div style="float: right;">
                        <s:Button ui:field="btnRefresh">Refresh</s:Button>
                    </div>
                </div>
            </g:HTMLPanel>

            <g:ScrollPanel ui:field="scrollPanel" height="450px">
        
                <g:FlexTable ui:field="table" width="100%" height="100%">
                </g:FlexTable>
     
            </g:ScrollPanel>
        </g:VerticalPanel>
    </g:HTMLPanel>
</ui:UiBinder>